{
    "name": "paresh/rsp-crud-generator",
    "description": "A Laravel package to generate API CRUD operations using the Repository-Service Pattern.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "RSPCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RSPCrud\\Providers\\RepositoryServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mrsoneri/rsp-crud-generator"
        }
    ],
    "authors": [
        {
            "name": "Paresh Soneri",
            "email": "soneriparesh435@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT"
}