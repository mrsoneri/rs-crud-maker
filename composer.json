{
    "name": "mrsoneri/laravel-make-resource",
    "description": "A Laravel package to generate resource files (repositories, services, controllers, etc.)",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MrSoneri\\MakeResource\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrSoneri\\MakeResource\\Providers\\MakeResourceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
