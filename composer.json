{
    "name": "mrsoneri/rs-crud-maker",
    "description": "A Laravel package to generate CRUD operations, repository, and service classes for resources",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "mrsoneri/rs-crud-maker": "*"
    },
    "autoload": {
        "psr-4": {
            "RsCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RsCrud\\Providers\\RsCrudMakerServiceProvider"
            ]
        }
    },
   "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mrsoneri/rs-crud-maker"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT"
}
